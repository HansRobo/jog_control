<launch>
  <include file="$(find ur_gazebo)/launch/ur5.launch"/>

  <include file="$(find ur5_moveit_config)/launch/ur5_moveit_planning_execution.launch">
    <arg name="sim" value="true"/>
    <arg name="limited" value="true"/>
  </include>

  <include file="$(find ur5_moveit_config)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
  </include>
  
  <node name="jog_frame_node" pkg="jog_controller" type="jog_frame_node">
    <param name="controller_name" value="arm_controller"/>
    <param name="target_link" value="wrist_3_link"/>
    <param name="group_name" value="manipulator"/>
  </node>

  <node if="false" name="joy" pkg="joy" type="joy_node">
    <param name="autorepeat_rate" value="100"/>
    <param name="deadzone" value="0.1"/>
  </node>
  <node if="false" name="joy_to_jog_frame" pkg="jog_controller" type="joy_to_jog_frame.py"/>
</launch>
